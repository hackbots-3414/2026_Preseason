<configuration>
 <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
   <encoder>
     <pattern>%date{mm:ss.SSS} [%thread] %highlight(%-5level) %logger{20} - %message %n</pattern>
   </encoder>
 </appender>

 <!--logger name="frc.robot.commands" level="INFO"/-->
 <!--logger name="frc.robot.subsystems.GamepieceFinder" level="TRACE"/-->

<!-- TRACE logs the finest level of detail, which can be overwhelming.  Best is to control at the class or package level -->
 <root level="TRACE">
   <appender-ref ref="STDOUT"/>
 </root>

</configuration>